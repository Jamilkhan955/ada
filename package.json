{
  "name": "ada",
  "version": "1.3.0",
  "description": "a simple frentend build tool",
  "main": "index.js",
  "bin": {
    "ada": "bin/ada.js"
  },
  "scripts": {
    "lint:css": "stylelint --cache -s scss --config .stylelintrc.js --config-basedir \"`npm get prefix`/lib/node_modules\" \"**/*.{scss,sass,css}\"",
    "lint:js": "eslint --cache --fix --ext .js,.jsx,.vue .",
    "lint": "npm run lint:js; npm run lint:css",
    "build": "node bin/ada.js build --cwd docs --noPx",
    "dev": "node bin/ada.js --cwd docs --noPx",
    "dev:hot": "node bin/ada.js --cwd docs --noPx --hotReact --hotVue",
    "test": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccbikai/ada"
  },
  "keywords": [
    "webpack",
    "postcss",
    "scss"
  ],
  "author": "ccbikai <ccbikai@qq.com> (http://miantiao.me/)",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.3",
    "handlebars-loader": "^1.7.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.3.11",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-spritesmith": "^0.5.3",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
