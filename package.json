{
  "name": "ada",
  "version": "1.1.2",
  "description": "a simple frentend build tool",
  "main": "index.js",
  "bin": {
    "ada": "bin/ada.js"
  },
  "scripts": {
    "lint:css": "stylelint --cache -s scss --config .stylelintrc.js --config-basedir \"`npm get prefix`/lib/node_modules\" \"**/*.{scss,sass,css}\"",
    "lint:js": "eslint --cache --fix --ext .js,.jsx,.vue .",
    "lint": "npm run lint:js; npm run lint:css",
    "build": "node bin/ada.js build --cwd docs --noPx",
    "dev": "node bin/ada.js --cwd docs --noPx",
    "dev:hot": "node bin/ada.js --cwd docs --noPx --hotReact --hotVue",
    "test": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccbikai/ada"
  },
  "keywords": [
    "webpack",
    "postcss",
    "scss"
  ],
  "author": "ccbikai <ccbikai@qq.com> (http://miantiao.me/)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.2",
    "handlebars-loader": "^1.7.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.0",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-spritesmith": "^0.4.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
